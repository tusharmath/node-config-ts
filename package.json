    "mocha": "^6.2.2",
    "semantic-release": "^15.13.30",
    "travis-deploy-once": "^5.0.9",
    "ts-node": "^8.4.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
